{
  "name": "diagonal",
  "version": "1.0.0",
  "description": "Diagonal - Full-stack construction company web application with admin dashboard, booking management, and photo uploads",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
